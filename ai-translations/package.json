{
  "name": "datocms-plugin-ai-translations",
  "homepage": "https://github.com/datocms/plugins/tree/master/ai-translations#readme",
  "version": "3.0.0",
  "author": "DatoCMS <support@datocms.com>",
  "description": "Translate fields and records directly on DatoCMS using AI",
  "keywords": [
    "datocms",
    "datocms-plugin"
  ],
  "type": "module",
  "datoCmsPlugin": {
    "title": "AI Translations",
    "coverImage": "docs/cover-1200x800.png",
    "previewImage": "docs/preview.mp4",
    "entryPoint": "dist/index.html",
    "permissions": [
      "currentUserAccessToken"
    ]
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@datocms/cma-client-browser": "^5.2.2",
    "@google/generative-ai": "^0.24.1",
    "datocms-plugin-sdk": "^2.0.9",
    "datocms-react-ui": "^2.0.9",
    "framer-motion": "^12.23.26",
    "locale-codes": "^1.3.1",
    "openai": "^6.15.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.4.0",
    "react-textarea-autosize": "^8.5.6",
    "react-type-animation": "^3.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.14.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-tsdoc": "^0.5.0",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "typescript": "^5.5.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
