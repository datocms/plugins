{
  "name": "datocms-plugin-delete-unused-assets",
  "homepage": "https://github.com/datocms/plugins/tree/master/delete-unused-assets#readme",
  "version": "0.2.0",
  "author": "DatoCMS <support@datocms.com>",
  "description": "A plugin that allows you to delete all unused assets with the click of a button.",
  "keywords": [
    "datocms",
    "datocms-plugin",
    "assets",
    "asset",
    "delete"
  ],
  "datoCmsPlugin": {
    "title": "Delete Unused Assets",
    "coverImage": "docs/cover.png",
    "entryPoint": "build/index.html",
    "permissions": [
      "currentUserAccessToken"
    ]
  },
  "dependencies": {
    "@datocms/cma-client-browser": "^1.3.5",
    "@types/node": "^18.16.18",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "datocms-plugin-sdk": "^0.7.14",
    "datocms-react-ui": "^0.7.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
    "build": "cross-env PUBLIC_URL='.' react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
