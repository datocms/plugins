{
  "name": "datocms-plugin-star-rating-editor",
  "version": "0.1.7",
  "homepage": "https://github.com/datocms/plugins/tree/master/star-rating-editor#readme",
  "description": "A plugin that presents integer fields as star rating widgets",
  "dependencies": {
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.11",
    "datocms-plugin-sdk": "^0.3.30",
    "datocms-react-ui": "^0.3.31",
    "lodash-es": "^4.17.21",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.7",
    "react-rating-stars-component": "^2.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.5.2"
  },
  "keywords": [
    "datocms",
    "datocms-plugin",
    "field-editor",
    "integer-field",
    "rating"
  ],
  "datoCmsPlugin": {
    "title": "Star rating editor",
    "previewImage": "docs/demo.mp4",
    "coverImage": "docs/cover.png",
    "entryPoint": "build/index.html",
    "permissions": []
  },
  "scripts": {
    "start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
    "build": "cross-env PUBLIC_URL='.' react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
