{
  "name": "datocms-plugin-web-previews",
  "homepage": "https://github.com/datocms/plugins/tree/master/web-previews#readme",
  "description": "Offer your editors some links to preview the record they're working on in the website frontend",
  "version": "1.0.4",
  "author": "DatoCMS <support@datocms.com>",
  "license": "GPLv3",
  "files": [
    "docs",
    "build"
  ],
  "keywords": [
    "datocms",
    "datocms-plugin",
    "sidebar",
    "preview",
    "links"
  ],
  "private": false,
  "datoCmsPlugin": {
    "title": "Web Previews",
    "previewImage": "docs/video.mp4",
    "coverImage": "docs/cover.png",
    "entryPoint": "build/index.html"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "datocms-plugin-sdk": "^0.6.15",
    "datocms-react-ui": "^0.6.15",
    "final-form": "^4.20.9",
    "final-form-arrays": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-scripts": "5.0.1",
    "use-deep-compare": "1.1.0"
  },
  "scripts": {
    "start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
    "build": "cross-env PUBLIC_URL='.' react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "typescript": "^4.9.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9"
  }
}
